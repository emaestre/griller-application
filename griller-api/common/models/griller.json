  "options": {
    "validateUpsert": true
  },
  "properties": {
    "grillerOwner": {
      "type": "string",
      "required": true
    },
    "reservedBy": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "isAvailable": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "bookingDate": {
      "type": "date"
    },
    "reservedHours": {
      "type": "number"
    }
